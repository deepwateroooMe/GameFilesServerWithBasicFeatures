@{
    ViewData["Title"] = "Upload";
}
<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="UploadFiles">
  <div class="form-group">
    <div class="col-md-12">
      <p>选择要上传的文件</p>
      <input type="file" name="files" multiple />
    </div>
  </div>
  <div class="form-group">
    <div class="col-md-12">
      <input type="submit" value="上传" />
    </div>
  </div>
</form>